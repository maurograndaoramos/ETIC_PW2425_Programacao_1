funcao somaDasNotas(lista):
    resultado = 0
    cada nota em lista:
        resultado = resultado + nota
    retorna resultado

funcao calculaMedia(notas):
    soma = somaDasNotas(notas)
    retorna soma/notas.tamanho()

notas = []
cada i em [1..3]:
    entrada("Coloca a {i}ยบ nota: ", x)
    notas.adiciona(x)

media = calculaMedia(notas)
se media >=7:
    imprime("Parabens! Aprovado!")
senao:
    imprime("Estudasses!")
